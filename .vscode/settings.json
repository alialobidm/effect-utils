{
  "deno.enable": true,
  "deno.lint": false,
  "[typescriptreact][typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[javascriptreact][javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "editor.codeActionsOnSave": {
    "quickfix.biome": "always",
    "source.organizeImports.biome": "always"
  }
}
